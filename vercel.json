{
  "version": 2,
  "functions": {
    "api/**/*.js": {
      "runtime": "@vercel/node"
    }
  },
  "routes": [
    {
      "src": "/api/auth/(.*)",
      "dest": "/api/auth.js"
    },
    {
      "src": "/api/services/(.*)",
      "dest": "/api/services.js"
    },
    {
      "src": "/api/portfolio/(.*)",
      "dest": "/api/portfolio.js"
    },
    {
      "src": "/api/careers/(.*)",
      "dest": "/api/careers.js"
    },
    {
      "src": "/api/blog/(.*)",
      "dest": "/api/blog.js"
    },
    {
      "src": "/api/contact/(.*)",
      "dest": "/api/contact.js"
    },
    {
      "src": "/api/admin/(.*)",
      "dest": "/api/admin.js"
    },
    {
      "src": "/api/health",
      "dest": "/api/health.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}